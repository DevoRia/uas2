fun bubbleSort(arr) {
    let n = len(arr)
    var outer = 0
    while outer < n - 1 {
        var inner = 0
        while inner < n - outer - 1 {
            if arr[inner] > arr[inner + 1] {
                let temp = arr[inner]
                arr[inner] = arr[inner + 1]
                arr[inner + 1] = temp
            }
            inner = inner + 1
        }
        outer = outer + 1
    }
}

var unsorted = [64, 34, 25, 12, 22, 11, 90]
print("  Before sort: " + unsorted)
bubbleSort(unsorted)
print("  After sort: " + unsorted)

fun binarySearch(arr, target) {
    var left = 0
    var right = len(arr) - 1
    
    while left <= right {
        let mid = int((left + right) / 2)
        
        if arr[mid] == target {
            return mid
        }
        
        if arr[mid] < target {
            left = mid + 1
        } else {
            right = mid - 1
        }
    }
    
    return -1
}

let position = binarySearch(unsorted, 25)
print("  Binary search 25: index " + position)
print("")

