fun factorial(n) {
    if n <= 1 {
        return 1
    }
    return n * factorial(n - 1)
}

fun fibonacci(n) {
    if n <= 1 {
        return n
    }
    return fibonacci(n - 1) + fibonacci(n - 2)
}

print("  5! = " + factorial(5))
print("  10! = " + factorial(10))

var fib_str = "  Fibonacci: "
var idx = 0
while idx < 10 {
    fib_str = fib_str + fibonacci(idx) + " "
    idx = idx + 1
}
print(fib_str)
print("")

